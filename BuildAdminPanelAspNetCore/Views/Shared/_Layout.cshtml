        function buildMenu(divId, items) {
            $.each(items, function (i, data) {
                //var li = $('<li class="nav-item treeview"><a href="' + this.formURL + '" class="nav-link" ><i class="' + this.menuImage + '"></i><p>' + this.menuName + ' <i class="right fas fa-angle-left"></i></p></a>');
                var li = $('<li class="nav-item treeview"><a href="' + this.formURL + '" class="nav-link" ><i class="' + this.menuImage + '"></i><p>' + this.menuName + '</p></a>');
                li.appendTo(divId);
                if (this.menuItemList && this.menuItemList.length > 0) {
                    var ul = $('<ul class="nav nav-treeview treeview-menu"></ul>');
                    ul.appendTo(li);
                    buildMenu(ul, this.menuItemList);
                }
            });
